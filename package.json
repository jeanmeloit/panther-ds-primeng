{
  "name": "pds-prime-ng",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:proxy": "ng serve --proxy-config=proxy.config.json",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "build:dev": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=dev",
    "build:homolog": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=homolog",
    "build:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "deploy:firebase": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=production && firebase deploy --only hosting",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint pds-prime-ng --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points --tsconfig \"./src/tsconfig.app.json\"",
    "conventional-changelog": "conventional-changelog",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "docs:build": "compodoc -p tsconfig.doc.json -d docs --language pt-BR",
    "docs:build-and-serve": "compodoc -p tsconfig.doc.json -d docs --language pt-BR -s",
    "docs:watch": "compodoc -p tsconfig.doc.json -d docs --language pt-BR -w -s",
    "docs:serve": "compodoc -d docs -s"
  },
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
